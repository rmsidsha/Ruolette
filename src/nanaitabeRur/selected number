// 초기 설정
private final static int SETTING_NUMBER = 5;


int radi = 360/SETTING_NUMBER;
for(int i = 0 ; i < SETTING_NUMBER ; i++) {
   if((rotation >= radi * i) && (rotation < (i + 1))) {
      tArea.append(i + "번이 당첨되었습니다!\n");
      tArea.setCaretPosition(tArea.getDocument().getLength());
   }
}


// 이벤트 처리
// 룰렛 변경 시 같이 변경

SETTING_NUMBER = 5;

SETTING_NUMBER = 6;

SETTING_NUMBER = 8;
